<Application
    x:Class="BarbellPro.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:BarbellPro.Application"
    xmlns:view="clr-namespace:BarbellPro.Application.Views"
    xmlns:viewModel="clr-namespace:BarbellPro.Application.ViewModels"
    StartupUri="MainWindow.xaml">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles/CalculatorRunButtonStyle.xaml" />
                <ResourceDictionary Source="Styles/CalculatorResetButtonStyle.xaml" />
                <ResourceDictionary Source="Styles/CalculatorTextBoxStyle.xaml" />
                <ResourceDictionary Source="Styles/ComboBoxStyle.xaml" />
                <ResourceDictionary Source="Styles/TextBoxStyles.xaml" />
                <ResourceDictionary Source="Styles/WindowBorderMinMaxButtonStyle.xaml" />
                <ResourceDictionary Source="Styles/WindowBorderExitButtonStyle.xaml" />
                <ResourceDictionary Source="Styles/NavigationPaneButtonStyle.xaml" />
                <ResourceDictionary Source="Resources/Images.xaml" />
                <ResourceDictionary Source="Utilities/DataTemplate.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
