<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:view="clr-namespace:BarbellPro.Application.Views"
    xmlns:viewModel="clr-namespace:BarbellPro.Application.ViewModels">

    <DataTemplate DataType="{x:Type viewModel:CalculatorViewModel}">
        <view:CalculatorView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel:LoadoutViewModel}">
        <view:LoadoutView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel:SettingsViewModel}">
        <view:SettingsView />
    </DataTemplate>

</ResourceDictionary>